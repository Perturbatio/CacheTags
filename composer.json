{
    "name": "perturbatio/cachetags",
    "description": "Provides a partial caching mechanism for use with Laravel's Blade templating language or in normal PHP with the helpers",
    "license": "MIT",
    "keywords": [
        "laravel",
        "blade",
        "cache",
        "partials"
    ],
    "authors": [
        {
            "name": "Kris Kelly",
            "email": "bertpotato@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Perturbatio\\CacheTags\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "4.*",
        "orchestra/testbench": "^3.3",
        "Illuminate/Container": "^5.3",
        "Illuminate/Filesystem": "^5.3"
    },
    "autoload-dev": {
        "psr-4": {
            "Perturbatio\\CacheTags\\Tests\\": "tests/"
        }
    },
    "require": {
        "Illuminate/Support": "^5.3",
        "Illuminate/Cache": "^5.3"
    },
    "scripts": {
        "test": "phpunit tests"
    },
    "branch-alias": {
        "dev-master": "1.0-dev"
    }
}
